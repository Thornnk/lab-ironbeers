<div class="beer-partial-big">
    <div>
        <img src={{image_url}} alt='beer-img'>
    </div>
    <div>
        <h3>{{name}}</h3>
        <p style="color: #bfbfbf">{{tagline}}</p>
        <p>{{description}}</p>
    </div>
    <h4>Food Pairing</h4>
    <ul>
    {{#each food_pairing}}
        <li>{{this}}</li>
    {{/each}}
    </ul>
    <p><strong>Brewer tips: </strong> {{brewers_tips}}</p>
</div>